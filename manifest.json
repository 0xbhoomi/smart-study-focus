{
  "manifest_version": 3,
  "name": "Smart Study Focus",
  "version": "1.0",
  "description": "Blocks YouTube and Instagram to help you focus on studying.",
  "permissions": ["declarativeNetRequest"],
  "host_permissions": [
    "*://*.youtube.com/*",
    "*://*.instagram.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Smart Study Focus"
  }
}
